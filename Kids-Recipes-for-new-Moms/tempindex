// following code block takes care of changing background color of col-md-1 on hover. before inserting following code, on hover the background color used to remain black even after hovering over other col-md-1 element.
$('.top-menu-row a.home').hover(function() {
	$(this).parent().css('background-color', '#A9A9A9');
	$(this).parent().siblings().css('background-color', '#696969');
});

$('.top-menu-row a.recipes').hover(function() {
    $(this).parent().css('background-color', '#A9A9A9');
    $(this).parent().siblings().css('background-color', '#696969');
});

$('.top-menu-row a.about').hover(function() {
    $(this).parent().css('background-color', '#A9A9A9');
    $(this).parent().siblings().css('background-color', '#696969');
});

$('.top-menu-row a.contactus').hover(function() {
    $(this).parent().css('background-color', '#A9A9A9');
    $(this).parent().siblings().css('background-color', '#696969');
});

// build the logic here for recent recipes- select 10 recipes from the JSON starting with the most recent

// recipes.meals[].datePosted
// recipes.snacks[].datePosted
// recipes.soupsDrinks[].datePosted
// recipes.sweetsDesserts[].datePosted

//add HTML elements dynamically. Assume that there are at least 2 items in each array type.
var mealsItemLength = recipes.meals.length, zeroMealsFlag = false;
var snacksItemLength = recipes.snacks.length, zeroSnacksFlag = false;
var soupsDrinksItemLength = recipes.soupsDrinks.length, zeroDrinksFlag = false;
var sweetsDessertsItemLength = recipes.sweetsDesserts.length, zeroDessertsFlag = false;
var tempMealsDateArray1, tempMealsDateArray2, twoOrMoreMealsFlag = false;
var tempSnacksDateArray1, tempSnacksDateArray2, twoOrMoreSnacksFlag = false;
var tempSoupsDrinksDateArray1, tempSoupsDrinksDateArray2, twoOrMoreDrinksFlag = false;
var tempSweetsDessertsDateArray1, tempSweetsDessertsDateArray2, twoOrMoreDessertsFlag = false;

//console.log(mealsItemLength, snacksItemLength, soupsDrinksItemLength, sweetsDessertsItemLength);

var h51 = '<h5 class="h51" style="font-family: Magra, sans-serif;">Recipe Name:%data%</h5>';
var h52 = '<h5 class="h52" style="font-family: Magra, sans-serif;">Category:%data%</h5>';
var h53 = '<h5 class="h53" style="font-family: Magra, sans-serif;">Date Posted:%data%</h5>';
var img = '<img class="img-responsive" src="http://lorempixel.com/800/300/food/3" alt="recipe">';
var recipesKeys = Object.keys(recipes);	//displays key names so as to add the name to category:



var column-name;
var tempColumnName = "row";
var inner-row, tempInnerRow = 1;
var inner-column, tempInnerColumn = 4;
var keyNumber = 0;
inner-row = tempInnerRow;    //initialize inner-row = 1; inner-row will change which is the actual inner-row number like 1 and 2 but tempInnerRow wont change
inner-column = tempInnerColumn;  //initialize inner-column = 4, same as above comment. 4 and 8

//use i to append to tempcolumn to use as rowID e.g. row + i = row1
for (i=1; i<=4; i++) {
	column-name = tempColumnName +i + inner-row + inner-column;			//this takes care of columns under inner-row1

	var formath51 = h51.replace("%data%", recipes.meals[mealsItemLength-1].name);
	var formath52 = h52.replace("%data%", recipesKeys[keyNumber]);
	var formath53 = h53.replace("%data%", recipes.meals[mealsItemLength-1].datePosted);

	$("column-name").append(formath51);
	$("column-name").append(formath52);
	$("column-name").append(formath53);
	inner-column = tempInnerColumn + 4;			
	column-name = tempColumnName + i + inner-row + inner-column;
	$("column-name").append(img);				//by the end of this code line, we display inner-row1 columns

	inner-row = tempInnerRow + 1; 				//inner-row2
	inner-column = tempInnerColumn;				//4
	column-name = tempColumnName + i + inner-row + inner-column;  //this takes care of columns under inner-row2
	var formath51 = h51.replace("%data%", recipes.meals[mealsItemLength-1].name);
	var formath52 = h52.replace("%data%", recipesKeys[keyNumber]);
	var formath53 = h53.replace("%data%", recipes.meals[mealsItemLength-1].datePosted);

	$("column-name").append(formath51);
	$("column-name").append(formath52);
	$("column-name").append(formath53);
	inner-column = tempInnerColumn + 4;			
	column-name = tempColumnName + i + inner-row + inner-column;
	$("column-name").append(img);				//by the end of this code line, we display inner-row2 columns

	//initialize inner-row=1, inner-column=4, keyNumber to fetch the next recipe group name from meals, snacks, soupsdrinks, sweetsdesserts etc.
	inner-row = tempInnerRow;
	inner-column = tempInnerColumn;
	keyNumber = keyNumber + 1;
}


